{
    "name": "swag-labs-behat-tests",
    "description": "Automated testing for Swag Labs e-commerce platform using Behat and Selenium.",
    "require": {
        "behat/behat": "^3.8",
        "behat/mink": "^1.8",
        "behat/mink-extension": "^2.3",
        "behat/mink-selenium2-driver": "^1.4",
        "symfony/yaml": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "bin-dir": "vendor/bin"
    }
}