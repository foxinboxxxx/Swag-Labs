{
    "name": "validaide/swaglabs",
    "description": "Automated testing for Swag Labs e-commerce platform using Behat and Selenium.",
    "require": {
        "behat/behat": "^3.8",
        "behat/mink": "^1.8",
        "behat/mink-extension": "^2.3",
        "behat/mink-selenium2-driver": "^1.3",
        "symfony/yaml": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "vendor/bin"
    },
    "scripts": {
        "test": "vendor/bin/behat"
    }
}